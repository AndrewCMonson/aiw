{
  "name": "@amdev/aiw",
  "version": "0.1.0",
  "description": "Repo-local prompt library CLI for Cursor workflows",
  "license": "MIT",
  "type": "module",
  "bin": {
    "aiw": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
